{
    "name": "sonsofphp/json",
    "type": "library",
    "description": "Wrapper around native json php functions",
    "keywords": ["json"],
    "homepage": "https://github.com/SonsOfPHP/json",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Estes",
            "email": "joshua@sonsofphp.com"
        }
    ],
    "support": {
